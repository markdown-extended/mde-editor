{
    "name": "markdown-extended/mde-editor",
    "description": "MarkdownExtended web editor (based on EpicEditor)",
    "homepage": "http://github.com/markdown-extended/mde-editor",
    "license": "MIT",
    "keywords": [
        "embeddable",
        "markdown",
        "markdown-extended",
        "editor"
    ],
    "authors": [{
        "name": "piwi",
        "homepage": "http://e-piwi.fr/"
    }],
    "require": {
        "php": ">=5.3.0",
        "piwi/markdown-extended": "dev-master",
        "markdown-extended/mde-service": "dev-master"
    },
    "replace": {
        "piwi/mde-editor": "*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.*-dev"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "bower install"
        ],
        "post-update-cmd": [
            "bower install"
        ]
    }
}
